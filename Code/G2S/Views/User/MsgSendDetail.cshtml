@{
    Layout = "~/Views/Shared/_Layout5.cshtml";
}
<script src="~/js/jquery-1.8.3.min.js"></script>
<script src="~/js/MsgSendDetail.js"></script>
<style>
    .Message {
        height: 35px;
        padding-top: 10px;
        padding-left: 25px;
    }

    .Frame {
        padding: 4px 20px 4px 20px;
        background: #475C7C;
        color: #FFF;
    }
</style>


<div>
    <input type="hidden" id="Temporary" value="" />
    <input type="hidden" id="Cont" value="" />
    <input type="hidden" id="MsgID" value="" />
    <div style=" width: 1000px; height: 41px; background: #374760">
        <a href="Msg" target="_self" style="float:left;line-height:41px;font-size:16px;font-weight:bold;margin-left:25px;color:#fff">我发送的消息</a>
        <span style="float:left;line-height:41px;font-size:16px;font-weight:bold;color:#fff">> 会议通知</span>
    </div>
    <div class="Message">
        <a href="javascript:GetReturn();" class="Frame">返回</a>
        <a href="javascript:replyInformation();" onclick="" class="Frame" style="margin-left:20px;">回复</a>
        <a href="javascript:Forward();" onclick="" class="Frame" style="margin-left:20px;">转发</a>
        <a href="javascript:OutMsgDel();" class="Frame" style="margin-left:20px;">删除</a>
        <p style="float:right; margin-right:20px;">
            <
            <a id="last" href="javascript:GetUpper();">上一封</a>
            <a id="next" href="javascript:GetNext();">下一封</a>
            >
        </p>
    </div>
    <div style="border-bottom: 1px solid #000000; float:left; width:100%;">
        <div style="width:100%; float:left;">
            <label id="Title" style=" margin-left:25px; font-size: 16px; font-weight: bold;"></label>
        </div>
        <div style="width:100%; float:left;">
            <label style=" margin-left:25px; float: left; color: #808080">发送人：</label>
            <label id="Name" style=" margin-left:10px; float: left; color: #808080"></label>
        </div>
        <div style="width:100%; float:left;">
            <label style=" margin-left:25px; float: left; color: #808080">&nbsp;&nbsp;&nbsp;时间：</label>
            <label id="Time" style=" margin-left:10px; float: left; color: #808080"></label>
        </div>
        <div style="width:100%; float:left;">
            <label style=" margin-left:25px; float: left; color: #808080">收件人：</label>
            <div id="Recipients" style=" margin-left:10px;float:left;width:900px; color: #808080"></div>
        </div>
    </div>
    <div id="Reply" style="display:none;">
        <div style="margin-left: 25px; margin-top: 15px; margin-right: 10px; width: 920px; border-bottom: 1px dashed #000000; ">
            <label>收到,谢谢！</label>
        </div>
        <div style="margin-left: 20px; width: 920px; height: 30px; border: 1px solid #F2F2F2; margin-top: 10px; padding-top: 5px; background-color: #F2F2F2; ">
            <label style=" margin-left:25px; float: left; color: #808080">发送人：</label>
            <label style=" margin-left:10px; float: left; color: #808080">张老师</label>
            <label style=" margin-left:25px; float: left; color: #808080">&nbsp;&nbsp;&nbsp;时间：</label>
            <label style=" margin-left:10px; float: left; color: #808080">2014-11-11</label>
        </div>
    </div>
    <div>
        <div id="Content" style="margin-left:25px; margin-top:15px; margin-right: 10px; width: 920px; height:200px;">
            @*@_mess.Conten*@
        </div>
    </div>
</div>
</html>
